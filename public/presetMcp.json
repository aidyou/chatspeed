    "website": "https://github.com/modelcontextprotocol/servers/tree/main/src/puppeteer",
    "config": {
      "mcpServers": {
        "puppeteer": {
          "command": "npx",
          "args": [
            "-y",
            "@modelcontextprotocol/server-puppeteer"
          ]
        }
      }
    }
  },
  {
    "name": "time",
    "description": "Provides time and timezone conversion capabilities. ",
