{
  "config": {
    "name": "Bing Search",
    "url_template": "https://www.bing.com/search?q={kw}&first={offset}&count={number}&filters={time_period}",
    "max_results_per_page": 25,
    "pagination_type": "offset",
    "pagination_param": "offset",
    "page_timeout": 15000,
    "wait_for": "#b_results",
    "wait_timeout": 10000
  },
  "selectors": {
    "base_selector": "#b_results > li.b_algo",
    "fields": [
      {
        "name": "title",
        "selector": "h2 a",
        "type": "text",
        "required": true
      },
      {
        "name": "url",
        "selector": "h2 a",
        "type": "attribute",
        "attribute": "href",
        "required": true
      },
      {
        "name": "snippet",
        "selector": "p",
        "type": "text"
      },
      {
        "name": "sitename",
        "selector": ".tilk",
        "type": "attribute",
        "attribute": "aria-label"
      }
    ]
  }
}
