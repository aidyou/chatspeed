{
  "config": {
    "name": "Sogou Search",
    "url_template": "https://www.sogou.com/web?query={kw}&page={page}&tsn={time_period}",
    "pagination_type": "page",
    "pagination_param": "page",
    "wait_for": "#main",
    "wait_timeout": 10000,
    "page_timeout": 15000,
    "max_results_per_page": 10
  },
  "selectors": {
    "base_selector": ".vrwrap",
    "fields": [
      {
        "name": "title",
        "selector": "h3.vr-title",
        "type": "text",
        "required": true
      },
      {
        "name": "url",
        "selector": "h3.vr-title a",
        "type": "attribute",
        "attribute": "href",
        "required": true
      },
      {
        "name": "snippet",
        "selector": ".space-txt",
        "type": "text"
      },
      {
        "name": "sitename",
        "selector": ".citeurl > span:first-child",
        "type": "text"
      },
      {
        "name": "publish_date",
        "selector": ".citeurl > span:last-child",
        "type": "text"
      }
    ]
  }
}