{
  "config": {
    "name": "wikipedia",
    "page_timeout": 15000,
    "wait_for": "body",
    "wait_timeout": 10000
  },
  "selectors": {
    "base_selector": "#content",
    "fields": [
      {
        "name": "title",
        "selector": "h1#firstHeading",
        "type": "text"
      },
      {
        "name": "content",
        "selector": "#mw-content-text",
        "type": "markdown"
      }
    ]
  }
}
